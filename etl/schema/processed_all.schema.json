{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "All MCI inhibitors summary (processed, aggregated by compound)",
  "type": "object",
  "properties": {
    "compound": {
      "type": "string",
      "minLength": 1
    },
    "pubmed_references": {
      "anyOf": [
        {
          "type": "integer",
          "minimum": 0
        },
        {
          "type": "string",
          "pattern": "^\\d+$"
        }
      ]
    },
    "known_status": {
      "type": "string",
      "enum": [
        "known",
        "new"
      ]
    },
    "confidence": {
      "type": "string"
    },
    "pubmed_ids": {
      "type": "string"
    },
    "SMILES": {
      "type": "string"
    },
    "MaxSim_all": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "string",
          "pattern": "^(?:\\d+(?:\\.\\d+)?|)$"
        }
      ]
    },
    "TopKMean_all": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "string",
          "pattern": "^(?:\\d+(?:\\.\\d+)?|)$"
        }
      ]
    },
    "BestRef_name": {
      "type": "string"
    },
    "confidence_similarity": {
      "type": "string"
    }
  },
  "required": [
    "compound",
    "pubmed_references",
    "known_status",
    "confidence",
    "pubmed_ids",
    "SMILES",
    "MaxSim_all",
    "TopKMean_all",
    "BestRef_name",
    "confidence_similarity"
  ],
  "additionalProperties": false
}
